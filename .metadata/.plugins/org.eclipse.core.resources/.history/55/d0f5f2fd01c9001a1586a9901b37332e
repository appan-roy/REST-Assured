			{"Brazil", "BR", "Milan"},
			{"Spain", "ES", "Madrid"},
			{"South Africa", "ZA", "Cape Town"},
			{"India", "IN", "New Delhi"},
			{"Australia", "AU", "Canberra"}
			
		};
		
	}
	
}
