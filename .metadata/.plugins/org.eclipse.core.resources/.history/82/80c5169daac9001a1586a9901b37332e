		baseURI = "http://coop.apps.symfonycasts.com";
		
		String accessToken = "";
		
		int userId = 1198;
		
		Response res = null;
		
		res = given().auth().oauth2(accessToken).post("	/api/"+userId+"/chickens-feed");
		
	}
	
	
	
}
