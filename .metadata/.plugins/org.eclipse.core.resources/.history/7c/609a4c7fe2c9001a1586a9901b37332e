package simplePayload;


public class Users {

	String firstName;
	String lastName;
	int fideRating;
	 
	public Users(String firstName, String lastName, int fideRating) {
		this.firstName=firstName;
		this.lastName=lastName;
		this.fideRating=fideRating;
	}
	 
	public String getfirstName() {
		return firstName;
	}
	 
	public void setfirstName(String firstName) {
		this.firstName = firstName;
	}
	 
	public String getlastName() {
		return lastName;
	}
	 
	public void setlastName(String lastName) {
		this.lastName = lastName;
	}
	 
	public Integer getfideRating() {
		return fideRating;
	}
	 
	public void setfideRating(Integer fideRating) {
		this.fideRating = fideRating;
	}
	 
    public String toString(){
    	return "The user's first name is " + this.firstName + " and last name is " + this.lastName + " and fide rating is " + this.fideRating; 
    }

}
